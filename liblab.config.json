{
  "sdkName": "test-sdk",
  "apiVersion": "1.0.0",
  "apiName": "test-api",
  "specFilePath": "https://raw.githubusercontent.com/OAI/OpenAPI-Specification/main/examples/v3.0/petstore.json",
  "languages": ["csharp", "typescript"],
  "createDocs": false,
  "auth": ["bearer"],
  "customizations": {
    "includeOptionalSnippetParameters": true,
    "authentication": {
      "access": {
        "prefix": "Bearer"
      }
    },
    "devContainer": false,
    "generateEnv": true,
    "inferServiceNames": false,
    "injectedModels": [],
    "license": {
      "type": "MIT"
    },
    "responseHeaders": false,
    "retry": {
      "enabled": true,
      "maxAttempts": 3,
      "retryDelay": 150
    }
  },
  "languageOptions": {
    "typescript": {
      "bundle": false,
      "exportClassDefault": false,
      "httpClient": "axios",
      "npmName": "",
      "npmOrg": "",
      "githubRepoName": "typescript-sdk",
      "ignoreFiles": [],
      "sdkVersion": "1.0.0"
    },
    "csharp": {
      "githubRepoName": "csharp-sdk",
      "ignoreFiles": []
    }
  },
  "liblabVersion": "2",
  "publishing": {
    "githubOrg": "skosijer"
  }
}
